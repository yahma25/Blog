{
    "env": {
        "node": true,
        "browser": true,
        "es6": true
    },
    "extends": ["airbnb", "airbnb-base", "plugin:@typescript-eslint/eslint-recommended"],
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "react",
        "@typescript-eslint"
    ],
    "rules": {
        "import/no-unresolved": 0
    }
}